{
    "networks": {
	"proxy": {
	    "subnet": "10.0.23.0/24",
	    "gateway": "10.0.23.7"
	},
	"upstream": {
	    "subnet": "10.0.73.0/24"
	},
	"network1": {},
	"network2": {},
	"other_network": {}
    },
    "volumes": {
	"/var/run/fatc/sockets/testvolume": {
	    "owner": "723:723",
	    "permissions": "0700"
	}
    },
    "daemons": {
	"daemon0": {
	    "healthy": "curl --output /dev/null --silent --head --fail --unix-socket $UPSTREAM http://localhost/",
	    "onstart": "./onstart",
	    "tracks": [
		{"listen": ":unix:",
		 "upstream": ":unix:"}
	    ],
	    "idle": {
		"timeout": 200,
		"test": "test-timeout"
	    }
	},
	"daemon1": {
	    "healthy": "curl --output /dev/null --silent --head --fail http://$UPSTREAM",
	    "tracks": [
		{"listen": ":unix:",
		 "upstream": ":upstream:http:80"}
	    ]
	},
	"daemon2": {
	    "depends_on": ["daemon3"],
	    "healthy": "curl --output /dev/null --silent --head --fail --unix-socket $UPSTREAM http://localhost/",
	    "tracks": [
		{"listen": "0.0.0.0:8082",
		 "upstream": ":unix:"}
	    ]
	},
	"daemon3": {
	    "healthy": "curl --output /dev/null --silent --head --fail http://$UPSTREAM",
	    "tracks": [
		{"listen": "0.0.0.0:8083",
		 "upstream": ":upstream:http_daemon3:80"}
	    ]
	}
    },
    "services": {
	"service0": {
	    "healthy": "curl --output /dev/null --silent --head --fail --unix-socket $UPSTREAM http://localhost",
	    "tracks": [
		{"listen": ":unix:",
		 "upstream": ":unix:"}
	    ],
	    "use_criu": ["http", "redis"],
	    "idle": "./last-activity"
	},
	"service1": {
	    "healthy": "curl --output /dev/null --silent --head --fail http://$UPSTREAM",
	    "idle": "./last-activity",
	    "custom-idle": "$COMPOSE exec http /usr/local/bin/last-activity",
	    "tracks": [
		{"listen": ":unix:",
		 "upstream": ":upstream:http_service1:80"}
	    ]
	},
	"service2": {
	    "healthy": "curl --output /dev/null --silent --head --fail --unix-socket $UPSTREAM http://localhost/",
	    "tracks": [
		{"listen": "0.0.0.0:8092",
		 "upstream": ":unix:"}
	    ]
	},
	"service3": {
	    "healthy": "curl --output /dev/null --silent --head --fail http://$UPSTREAM",
	    "tracks": [
		{"listen": "0.0.0.0:8093",
		 "upstream": ":upstream:http_service3:80"}
	    ]
	},
	"service4": {
	    "healthy": "curl --output /dev/null --silent --head --fail http://$UPSTREAM",
	    "tracks": [
		{"listen": ":unix:",
		 "upstream": ":backend:50001"}
	    ]
	},
	"service5": {
	    "healthy": "curl --output /dev/null --silent --head --fail http://$UPSTREAM",
	    "tracks": [
		{"listen": "0.0.0.0:8095",
		 "upstream": ":backend:50001"}
	    ]
	}
    }
}
