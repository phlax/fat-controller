[controller]
context: ${PWD}/example
idle-timeout: 10
name: example
# move this!
apps: ctrl.zmq
      ctrl.compose
      ctrl.systemd
zmq-listen: ipc:///controller/sockets/zmq-rpc.socket
zmq-publish: ipc:///controller/sockets/zmq-subscriber.socket ctrl

[service:nginx]
description: Containerized: The NGINX HTTP and reverse proxy server
listen: 0.0.0.0:8080
idle-files: /var/log/nginx/nginx.log
            /var/log/nginx/error.log
